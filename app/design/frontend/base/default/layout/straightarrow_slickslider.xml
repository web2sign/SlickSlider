<layout version="0.1.0">
  <default>
    <reference name="head">
      <action method="addItem">
        <type>skin_js</type>
        <name>sac_slider/slick/slick.min.js</name>
      </action>
      <action method="addItem">
        <type>skin_css</type>
        <name>sac_slider/slick/slick.min.css</name>
      </action>
      <action method="addItem">
        <type>skin_css</type>
        <name>sac_slider/slick/slick-theme.css</name>
      </action>
      <action method="addItem">
        <type>skin_css</type>
        <name>sac_slider/slick/custom.css</name>
      </action>
    </reference>

    <reference name="header">
      <block type="straightarrow_slickslider/slickslider"  name="straightarrow_slickslider_slider" as="slickSliderHeader" template="straightarrow/slickslider/slider.phtml" />
    </reference>

    <reference name="content">
      
    </reference>


    <reference name="footer">
      
    </reference>

  </default>
  <sliderfrontend_index_index>

  </sliderfrontend_index_index>
</layout>